block append style
	+stylesheet('/style/tutoren.css')

mixin Tutor(tutor) 
	-
		var image = `/images/tutoren/${tutor.Name}.jpg`;
	.tutor
		img(src=encodeURI(image), alt='')
		div
			h3= tutor.Name
			p= tutor.Spruch
			br
			each Studium in tutor.Karriere
				- 
				-
					const Studiengang = Studium.Studiengang;
					const Semester = Studium => {
						const years = new Date().getFullYear() - Studium.Jahr;
						const realSemesters = 2 * years - (new Date().getMonth() < 10 ? 1 : 0);
						return realSemesters - (Studium.Sommersemester ? 1 : 0) - (Studium.Urlaubssemester ?? 0) + 1;
					}
				p #{Studiengang}
					br
					| #{Semester(Studium)}. Semester
				br

block main
	+section('Tutor:innen')
	p Wir sind die Menschen, die dich durch deine O-Phase begleiten werden. Falls du irgendwelche Fragen hast, kannst du uns jederzeit eine Email an #[+maillink('tutoren@studierlangsam.de')] schreiben.
	
	- var tutors = require("./script/tutors.js");
	each tutor in tutors.loadTutors()
		+Tutor(tutor)

